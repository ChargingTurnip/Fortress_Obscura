	"%tooltexture" "dev/tfwater_normal"
	"%compilewater" 1
	"$abovewater" 1
//	"$nofresnel" "1"

	"$envmap" "env_cubemap"	
	"$refracttexture" "_rt_WaterRefraction"

	"$refractamount" "0.6"

	"$refracttint" "{184 191 208}"
	"$refractblur" "0"

//	"$reflecttexture" "_rt_WaterReflection"
//	"$reflectamount" ".1"
//	"$reflecttint" "{230 240 255}"
//	"$reflectsaturation" "[0 0 1]"

	"$scale" "[1 1]"

	"$bumpmap" "water/dx80_tfwater001_dudv"
	"$normalmap" "water/dx80_tfwater001_normal"

	"$surfaceprop" "water"
	"$bottommaterial" "water/water_well_beneath.vmt"
	"$bumpframe" "0"

	"$fogenable" 1
	"$fogcolor" "{24 38 53}"
	"$fogstart" "1"
	"$fogend" "300"

	 "$temp" "[0 0]"
	 "$curr" 0.0
	 "$curr2" 0.0



	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}

		"Sine"
		{
			"sineperiod" "24"
			"sinemin" -0.5
			"sinemax" 0.5
			"resultVar" "$curr"
		}
		"Sine"
		{
			"sineperiod" "16"
			"sinemin" 0.5
			"sinemax" -0.5
			"resultVar" "$curr2"
		}

		"Equals"
		{
			"srcVar1" "$curr2"
			"resultVar" "$temp[0]"
		}
		"Equals"
		{
			"srcVar1" "$curr"
			"resultVar" "$temp[1]"
		}

 
		"TextureTransform"
		{
			"translateVar" "$temp"
			"resultVar" "$bumptransform"
		}  

