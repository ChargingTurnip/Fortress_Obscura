"UnlitTwoTexture"
{
	"$basetexture" "effects/tiledfire/fireLayeredSlowTiled512.vtf"
	"$texture2" "effects/imcookin_2"
//	"$texture2" "effects/tiledfire/fireLayeredSlowTiled512.vtf"
	"$additive" "1"
	"$burnlevel" "0"
	"$color" "[1 1 1]"
	"Proxies"
	{

		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 40
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$texture2"
			"animatedtextureframenumvar" "$frame2"
			"animatedtextureframerate" 40
		}

		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" ".5"
			"texturescrollangle" "90"
		}
		"BurnLevel"
		{
			"resultVar" "$burnlevel"
		}

		"Equals"
		{
			"srcVar1" "$burnlevel"
			"resultVar" "$color[0]"
		}
		"Equals"
		{
			"srcVar1" "$burnlevel"
			"resultVar" "$color[1]"
		}
		"Equals"
		{
			"srcVar1" "$burnlevel"
			"resultVar" "$color[2]"
		}
		


	}

}
