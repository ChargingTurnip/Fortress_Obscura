"UnlitGeneric"
{
	"$basetexture" "sprites/glow_test02"
	$additive 1
	$vertexcolor 1
	$vertexalpha 1

       "$angle" 180.0
        "$translate" "[0.0 -0.05]"
        "$center" "[0.5 0.5]"
        "Proxies"
        {


            "TextureTransform"
            {
                "translateVar" "$translate"
                "rotateVar" "$angle"
                "centerVar" "$center"
                "resultVar" "$basetexturetransform"
            }
        }

}
