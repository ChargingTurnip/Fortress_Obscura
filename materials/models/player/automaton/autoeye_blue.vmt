"VertexlitGeneric"
{
	"$basetexture" "models/player/automaton/autoeye_blue"
	"$surfaceprop" "Metal"
	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "2"	
	"$phongfresnelranges"	"[.5 .7 1]"
	"$halflambert" "0"

	"$lightwarptexture" "models/lightwarps/robot_lightwarp"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "1"				
	"$rimlightboost" "2"
	"$rimmask" "1"


	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
	}
	"%keywords" "props prop player model models tf"
}
