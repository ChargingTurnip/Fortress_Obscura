
"VertexLitGeneric"
{
	"$basetexture" "models/props_badlands/blue"
	"$selfillum" 1
	//"$texture2"	"dev/dev_scanline"
	"$mod2x" "1"


	"Proxies"
	{



		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 12
		}


		"TextureScroll"
		{
			"texturescrollvar" "$basetexture"
			"texturescrollrate" .16
			"texturescrollangle" -90
		}

	}
}
